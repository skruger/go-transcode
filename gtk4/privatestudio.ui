<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.16.0 -->
<interface>
  <!-- interface-name privatestudio.ui -->
  <requires lib="gtk" version="4.6"/>
  <object class="GtkApplicationWindow" id="psMainWindow">
    <property name="default-height">800</property>
    <property name="default-width">1000</property>
    <property name="show-menubar">True</property>
    <property name="title">Private Studio</property>
    <child>
      <object class="GtkBox">
        <property name="orientation">vertical</property>
        <child>
          <object class="GtkBox">
            <property name="orientation">vertical</property>
            <property name="vexpand">True</property>
            <child>
              <object class="GtkBox">
                <property name="hexpand">True</property>
                <property name="orientation">vertical</property>
                <child>
                  <object class="GtkBox">
                    <child>
                      <object class="GtkButton" id="openFileBtn">
                        <property name="label">Open File</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkListView" id="sourcesList">
                    <property name="vexpand">True</property>
                  </object>
                </child>
              </object>
            </child>
            <child>
              <object class="GtkSeparator">
                <property name="margin-end">2</property>
                <property name="margin-start">2</property>
              </object>
            </child>
            <child>
              <object class="GtkBox">
                <property name="hexpand">True</property>
                <property name="hexpand-set">True</property>
                <property name="orientation">vertical</property>
                <property name="width-request">400</property>
                <child>
                  <object class="GtkBox">
                    <child>
                      <object class="GtkDropDown" id="transcodeProfileDropDown"/>
                    </child>
                    <child>
                      <object class="GtkButton" id="transcodeBtn">
                        <property name="icon-name">media-record</property>
                        <property name="label">Transcode</property>
                        <property name="tooltip-text">Transcode Asset</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="packageBtn">
                        <property name="icon-name">package-x-generic</property>
                        <property name="tooltip-text">Package Asset</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkButton" id="distributeBtn">
                        <property name="icon-name">document-send</property>
                        <property name="tooltip-text">Distribute Asset</property>
                        <property name="visible">False</property>
                      </object>
                    </child>
                  </object>
                </child>
                <child>
                  <object class="GtkGrid">
                    <property name="column-spacing">3</property>
                    <property name="row-spacing">2</property>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">start</property>
                        <property name="label">Filename</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="column-span">1</property>
                          <property name="row">0</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="labelFilename">
                        <property name="hexpand">True</property>
                        <property name="xalign">0.0</property>
                        <layout>
                          <property name="column">1</property>
                          <property name="column-span">1</property>
                          <property name="row">0</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">start</property>
                        <property name="label">Codec</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="column-span">1</property>
                          <property name="row">1</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">start</property>
                        <property name="label">File Size</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="column-span">1</property>
                          <property name="row">2</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">start</property>
                        <property name="label">Duration</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="column-span">1</property>
                          <property name="row">3</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel">
                        <property name="halign">start</property>
                        <property name="label">Resolution</property>
                        <layout>
                          <property name="column">0</property>
                          <property name="column-span">1</property>
                          <property name="row">4</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="labelCodec">
                        <property name="hexpand">True</property>
                        <property name="xalign">0.0</property>
                        <layout>
                          <property name="column">1</property>
                          <property name="column-span">1</property>
                          <property name="row">1</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="labelSize">
                        <property name="hexpand">True</property>
                        <property name="xalign">0.0</property>
                        <layout>
                          <property name="column">1</property>
                          <property name="column-span">1</property>
                          <property name="row">2</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="labelDuration">
                        <property name="hexpand">True</property>
                        <property name="xalign">0.0</property>
                        <layout>
                          <property name="column">1</property>
                          <property name="column-span">1</property>
                          <property name="row">3</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <child>
                      <object class="GtkLabel" id="labelResolution">
                        <property name="hexpand">True</property>
                        <property name="xalign">0.0</property>
                        <layout>
                          <property name="column">1</property>
                          <property name="column-span">1</property>
                          <property name="row">4</property>
                          <property name="row-span">1</property>
                        </layout>
                      </object>
                    </child>
                    <style>
                      <class name="fileinfoGrid"/>
                    </style>
                  </object>
                </child>
                <child>
                  <object class="GtkStackSwitcher">
                    <property name="stack">outputStack</property>
                  </object>
                </child>
                <child>
                  <object class="GtkStack" id="outputStack">
                    <child>
                      <object class="GtkStackPage">
                        <property name="child">
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkListView" id="outputList">
                                <property name="vexpand">True</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <child>
                                  <object class="GtkButton" id="transcodeOutputDelete">
                                    <property name="icon-name">user-trash</property>
                                    <property name="label">Delete Output</property>
                                    <property name="tooltip-text">Delete Transcode Output</property>
                                  </object>
                                </child>
                                <child>
                                  <object class="GtkBox">
                                    <property name="orientation">vertical</property>
                                    <child>
                                      <object class="GtkBox"/>
                                    </child>
                                    <child>
                                      <object class="GtkSeparator">
                                        <property name="margin-end">2</property>
                                        <property name="margin-start">2</property>
                                        <property name="orientation">vertical</property>
                                      </object>
                                    </child>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </property>
                        <property name="name">transcodeOutput</property>
                        <property name="title">Transocde Outputs</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="child">
                          <object class="GtkBox">
                            <property name="orientation">vertical</property>
                            <child>
                              <object class="GtkListView">
                                <property name="vexpand">True</property>
                              </object>
                            </child>
                            <child>
                              <object class="GtkBox">
                                <child>
                                  <object class="GtkButton" id="deletePakageBtn">
                                    <property name="label">Delete Package</property>
                                  </object>
                                </child>
                              </object>
                            </child>
                          </object>
                        </property>
                        <property name="name">packages</property>
                        <property name="title">Packages</property>
                      </object>
                    </child>
                    <child>
                      <object class="GtkStackPage">
                        <property name="child">
                          <object class="GtkLabel">
                            <property name="label">Empty Page</property>
                          </object>
                        </property>
                        <property name="name">distributions</property>
                        <property name="title">Distributions</property>
                        <property name="visible">False</property>
                      </object>
                    </child>
                  </object>
                </child>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="GtkStatusbar" id="statusBar"/>
        </child>
      </object>
    </child>
  </object>
</interface>
